<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
 >
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
            >
             <Key
                latin:keyLabel="&#x1007;"
                latin:keyLabelFlags="fontNormal" />
             <Key
                latin:keyLabel="&#x100C;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
             <Key
                latin:keyLabel="&#x1003;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
             <Key
                latin:keyLabel="&#x1020;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
             <Key
                latin:keyLabel="&#x101A;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
             <Key
                latin:keyLabel="&#x1009;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
             <Key
                latin:keyLabel="&#x1026;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
             <Key
                latin:keyLabel="&#x104A;"
                latin:keyLabelFlags="fontNormal" />
        </case>
        <default>
             <Key
                latin:keyLabel="&#x1016;"
                latin:keyLabelFlags="fontNormal" />
             <Key
                latin:keyLabel="&#x1011;"
                latin:keyLabelFlags="fontNormal" />
             <Key
                latin:keyLabel="&#x1001;"
                latin:keyLabelFlags="fontNormal" />
             <Key
                latin:keyLabel="&#x101C;"
                latin:keyLabelFlags="fontNormal" />
             <Key
                latin:keyLabel="&#x1018;"
                latin:keyLabelFlags="fontNormal" />
             <Key
                latin:keyLabel="&#x100A;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
             <Key
                latin:keyLabel="&#x102C;"
                latin:keyLabelFlags="fontNormal" />
             <Key
                latin:keyLabel="&#x104B;"
                latin:keyLabelFlags="fontNormal" />
        </default>
    </switch>
</merge>
