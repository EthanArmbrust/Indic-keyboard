<?xml version="1.0" encoding="utf-8"?>

<merge
    xmlns:latin="http://schemas.android.com/apk/res/org.smc.inputmethod.indic"
 >
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
            >
            <!-- U+0C9F: "ಠ" kannada TTA
                 +0CA0: "ಠ   Kannada TTHA  -->
            <Key
                latin:keyLabel="&#x0A94;"
                latin:keyLabelFlags="fontNormal" />
            <!-- U+0CA1: "ಡ" kannada DA
                 +0CA2: "ಢ"   Kannada DHA  -->
            <Key
                latin:keyLabel="&#x0A90;"
                latin:keyLabelFlags="fontNormal" />
            <!-- E: matras, short E, long E
                 Vowels and Consonats  -->
            <Key
                latin:keyLabel="&#x0A86;"
                latin:keyLabelFlags="fontNormal" />
            <!-- R: ra, vocalic R matra, vocalic R, RRA
                 Vowels and Consonats  -->
            <Key
                latin:keyLabel="&#x0A88;"

                latin:keyLabelFlags="fontNormal" />
            <!-- T: dental ta, tha -->
            <Key
                latin:keyLabel="&#x0A8A;"
                latin:keyLabelFlags="fontNormal" />
            <!-- // Y: ya, ai matra, ai -->
            <Key
                latin:keyLabel="&#x0AAD;"
                latin:keyLabelFlags="fontNormal" />
            <!-- // U: matras, u, uu -->
            <Key
                latin:keyLabel="&#x0A99;"
                latin:keyLabelFlags="fontNormal" />
            <!-- // // I: matras, i, ii -->
            <Key
                latin:keyLabel="&#x0A98;"

                latin:keyLabelFlags="fontNormal" />
            <!--  // O: matras, short o, long o -->
            <Key
                latin:keyLabel="&#x0AA7;"
                latin:keyLabelFlags="fontNormal" />
            <!--  // O: P: pa, pha -->
            <Key
                latin:keyLabel="&#x0A9D;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0AA2;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0A9E;"
                latin:keyLabelFlags="fontNormal" />
        </case>

        <default>
            <!-- U+0C9F: "ಠ" kannada TTA
                 +0CA0: "ಠ   Kannada TTHA  -->
            <Key
                latin:keyLabel="&#x0ACC;"
                latin:keyLabelFlags="fontNormal" />
            <!-- U+0CA1: "ಡ" kannada DA
                 +0CA2: "ಢ"   Kannada DHA  -->
            <Key
                latin:keyLabel="&#x0648;"
                latin:keyLabelFlags="fontNormal" />
            <!-- E: matras, short E, long E
                 Vowels and Consonats  -->
            <Key
                latin:keyLabel="&#x0ABE;"
                latin:keyLabelFlags="fontNormal" />
            <!-- R: ra, vocalic R matra, vocalic R, RRA
                 Vowels and Consonats  -->
            <Key
                latin:keyLabel="&#x0AC0;"
                latin:keyLabelFlags="fontNormal" />
            <!-- T: dental ta, tha -->
            <Key
                latin:keyLabel="&#x0AC2;"
                latin:keyLabelFlags="fontNormal" />
            <!-- // Y: ya, ai matra, ai -->
            <Key
                latin:keyLabel="&#x0AAC;"
                latin:keyLabelFlags="fontNormal" />
            <!-- // U: matras, u, uu -->
            <Key
                latin:keyLabel="&#x0AB9;"
                latin:keyLabelFlags="fontNormal" />
            <!-- // // I: matras, i, ii -->
            <Key
                latin:keyLabel="&#x0A97;"
                latin:keyLabelFlags="fontNormal" />
            <!--  // O: matras, short o, long o -->
            <Key
                latin:keyLabel="&#x0AA6;"
                latin:keyLabelFlags="fontNormal" />
            <!--  // O: P: pa, pha -->
            <Key
                latin:keyLabel="&#x0A9C;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0AA1;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keyLabel="&#x0ABC;"
                latin:keyLabelFlags="fontNormal" />
        </default>
    </switch>
</merge>
